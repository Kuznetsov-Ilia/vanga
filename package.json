{
  "name": "my-vanga",
  "version": "1.1.7",
  "description": "dom templator with plugins for gulp, webpack, browserify, rollup",
  "author": "Ilia Kuznetsov",
  "main": "index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "index.js",
    "events",
    "plugins",
    "src",
    "tags",
    "template",
    "parser.js"
  ],
  "scripts": {
    "gen": "gulp gen",
    "start": "rollup -c rollup.config.js & rollup -f cjs -i src/parser.js -o parser.js & rollup -f cjs -i src/parser.js -o parser.js",
    "deploy": "git add . && git commit -am 'fix' && git push && npm publish",
    "test": "babel-node test | faucet",
    "test-bro": "browserify ./test/index.js > test/bundle.js"
  },
  "dependencies": {
    "my-global": "^1.0.0",
    "my-util": "^1.0.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "left-pad": "*"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "faucet": "*",
    "gulp": "*",
    "gulp-beautify": "*",
    "gulp-eslint": "*",
    "gulp-mocha": "*",
    "gulp-rename": "*",
    "gulp-spawn-shim": "*",
    "gulp-util": "*",
    "jsdom": "*",
    "rollup": "*",
    "rollup-plugin-node-resolve": "*",
    "rollup-pluginutils": "*",
    "sax": "*",
    "tap-nyan": "*",
    "tape": "*",
    "through": "*",
    "through2": "*",
    "uglify-js": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QiV/vanga"
  },
  "keywords": [],
